{
  "description": "Angular controller and partial",
  "files": [
    {
      "from": "template.html.hbs",
      "to": "app/partials/{{name}}/template.html"
    },
    {
      "from": "style.styl.hbs",
      "to": "app/partials/{{name}}/style.styl"
    }
  ],
  "dependencies": [
    { "name": "controller", "params": "{{name}}" }
  ]
}
